<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="comments">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="row"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="row">
    <xs:complexType>
      <xs:attribute name="CreationDate" use="required" type="xs:NMTOKEN"/>
      <xs:attribute name="Id" use="required" type="xs:integer"/>
      <xs:attribute name="PostId" use="required" type="xs:integer"/>
      <xs:attribute name="Score" use="required" type="xs:integer"/>
      <xs:attribute name="Text" use="required"/>
      <xs:attribute name="UserId" use="required" type="xs:integer"/>
    </xs:complexType>
  </xs:element>
</xs:schema>
