			<Title>
			"Dry hopping versus regular hopping"
			</Title>
			<Tags>
				<tag>hops</tag>
			</Tags>
			<Credit> 
				<Score>9</Score>
				<ViewCount>207</ViewCount>
				<AnswerCount>1</AnswerCount>
