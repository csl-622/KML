			<PostId>243</PostId>
			<Title>
			"Popularity of tank beer"
			</Title>
			<Tags>
				<tag>storage</tag>
				<tag>draught</tag>
				<tag>restaurants</tag>
			</Tags>
			<Credit> 
				<Score>7</Score>
