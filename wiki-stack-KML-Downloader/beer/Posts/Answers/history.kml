		<Instance Id="55" InstanceType="answer">
			<PostId>1111</PostId>
			<ParentPostId>1098</ParentPostId>			<Tags>
				<tag>history</tag>
				<tag>pairing</tag>
			</Tags>
			<Credit> 
				<Score>0</Score>
