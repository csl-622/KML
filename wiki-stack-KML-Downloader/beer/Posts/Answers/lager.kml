		<Instance Id="23" InstanceType="answer">
			<PostId>4538</PostId>
			<ParentPostId>4537</ParentPostId>			<Tags>
				<tag>taste</tag>
				<tag>style</tag>
				<tag>stout</tag>
				<tag>lager</tag>
