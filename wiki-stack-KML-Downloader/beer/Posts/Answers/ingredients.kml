		<Instance Id="6" InstanceType="answer">
			<PostId>165</PostId>
			<ParentPostId>35</ParentPostId>			<Tags>
				<tag>brewing</tag>
				<tag>ingredients</tag>
			</Tags>
			<Credit> 
				<Score>3</Score>
