		<Instance Id="16" InstanceType="answer">
			<PostId>6896</PostId>
			<ParentPostId>6895</ParentPostId>			<Tags>
				<tag>liquor</tag>
				<tag>consumption</tag>
			</Tags>
			<Credit> 
				<Score>4</Score>
