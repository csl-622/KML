		<Instance Id="41" InstanceType="answer">
			<PostId>4756</PostId>
			<ParentPostId>4755</ParentPostId>			<Tags>
				<tag>taste</tag>
				<tag>style</tag>
				<tag>serving</tag>
				<tag>glassware</tag>
				<tag>pairing</tag>
