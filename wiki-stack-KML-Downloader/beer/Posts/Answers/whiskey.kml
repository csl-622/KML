		<Instance Id="49" InstanceType="answer">
			<PostId>6875</PostId>
			<ParentPostId>6868</ParentPostId>			<Tags>
				<tag>whiskey</tag>
				<tag>spirits</tag>
				<tag>scotch</tag>
			</Tags>
			<Credit> 
				<Score>1</Score>
