		<Instance Id="86" InstanceType="answer">
			<PostId>6761</PostId>
			<ParentPostId>6758</ParentPostId>			<Tags>
				<tag>flavor</tag>
			</Tags>
			<Credit> 
				<Score>4</Score>
				<CommentCount>0</CommentCount>
